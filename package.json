{
  "name": "ldap-query-generator",
  "version": "0.2.2",
  "description": "LDAP query generator",
  "repository": {
    "type": "git",
    "url": "https://github.com/saostad/ldap-query-generator"
  },
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "format": "prettier --check \"src/**/*.ts\" --write",
    "lint": "eslint --ext \".ts,.js\" src --quiet",
    "gen-docs": "typedoc",
    "test": "jest",
    "test:watch": "jest --watch",
    "tsc": "tsc",
    "tsc:watch": "tsc --watch",
    "prepublishOnly": "npm run tsc",
    "prod": "node --unhandled-rejections=strict dist/src/app.js",
    "dev": "nodemon --watch dist --exec \"npm run prod\"",
    "start": "npm-run-all -p dev tsc:watch"
  },
  "keywords": [
    "ldap",
    "typescript",
    "query",
    "generator",
    "schema",
    "aware"
  ],
  "author": "Saeid Ostad",
  "license": "MIT",
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "docs/*"
    ],
    "delay": "1000"
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/node": "^13.11.1",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^25.3.0",
    "nodemon": "^2.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "ts-jest": "^25.3.1",
    "ts-node": "^8.6.2",
    "typedoc": "^0.17.4",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "fast-node-logger": "^1.3.4"
  }
}
